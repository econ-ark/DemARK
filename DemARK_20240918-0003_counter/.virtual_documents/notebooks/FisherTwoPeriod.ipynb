    )
    plt.plot(C1_opt_RHi, C2_opt_RHi, "mo", label="C: Optimal Consumption R High")
    plt.plot(C1HW, C2HW, "co", label="D: HW effect AD")

    plt.legend()
    plt.show()

    return None


